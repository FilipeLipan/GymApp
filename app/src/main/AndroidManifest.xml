<?xml version="1.0" encoding="utf-8"?>
<manifest package="filipelipan.com.github.academia"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.CreateTrainingActivity"
            android:label="@string/title_activity_create_training">
        </activity>
        <activity
            android:name=".ui.ViewExerciseActivity"
            android:label="@string/title_activity_view_exercise">
        </activity>
    </application>

</manifest>
